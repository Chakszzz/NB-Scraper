{
  "entryPoints": ["app/index.ts"],
  "out": "docs",
  "theme": "appium",
  "name": "NB Scraper Documentation",
  "includeVersion": true,
  "entryPointStrategy": "packages",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "tsconfig": "./tsconfig.json",
  "readme": "readme.md",
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Scrapers",
    "Types",
    "Utilities",
    "*"
  ],
  "sort": ["source-order"],
  "kindSortOrder": [
    "Project",
    "Module",
    "Namespace",
    "Enum",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Variable",
    "Function",
    "Accessor",
    "Method"
  ],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "plugin": [
    "typedoc-plugin-missing-exports",
    "typedoc-plugin-mdn-links",
    "typedoc-theme-hierarchy"
  ],
  "githubPages": true,
  "hideGenerator": true,
  "searchInComments": true,
  "cleanOutputDir": true
}
