{
  "entryPoints": ["app/index.ts"],
  "out": "docs",
  "cname": "nb-scraper.js.org",
  "name": "NB Scraper Docs",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "theme": "my-theme",
  "readme": "docs.md",
  "favicon": "public/favicon.ico",
  "categorizeByGroup": false,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Scrapers",
    "Types",
    "Utilities",
    "*"
  ],
  "sort": ["source-order", "kind", "static-first"],
  "kindSortOrder": [
    "Project",
    "Module",
    "Namespace",
    "Enum",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Variable",
    "Function",
    "Accessor",
    "Method"
  ],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "modifierTags": [
    "@alpha", 
    "@beta", 
    "@experimental"
  ],
  "markdownItOptions": {
    "html": true
  },
  "plugin": [
    "typedoc-plugin-missing-exports",
    "typedoc-plugin-mdn-links",
    "@mxssfd/typedoc-theme"
  ],
  "githubPages": true,
  "hideGenerator": true,
  "searchInComments": true,
  "cleanOutputDir": true
}